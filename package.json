{
  "name": "micro-emitter",
  "version": "1.1.15",
  "description": "micro event emitter.",
  "main": "lib/index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prepare": "npm test && npm run build",
    "build": "tsc --declaration",
    "watch": "tsc --declaration -w",
    "lint": "eslint '{src,cli}/**/*.{ts,tsx,js,jsx,json}'",
    "lint:fix": "eslint --fix '{src,cli}/**/*.{ts,tsx,js,jsx,json}'",
    "format": "prettier --ignore-path .gitignore --config .prettierrc --write 'src/**/*'",
    "test": "ava",
    "start": "npm run build",
    "check": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khirayama/micro-emitter.git"
  },
  "keywords": [
    "eventemitter",
    "emitter",
    "event",
    "events",
    "flux"
  ],
  "author": "khirayama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khirayama/micro-emitter/issues"
  },
  "homepage": "https://github.com/khirayama/micro-emitter#readme",
  "devDependencies": {
    "@types/sinon": "7.0.9",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "ava": "1.3.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-espower": "3.0.1",
    "babel-preset-es2015": "6.24.1",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "npm-check-updates": "2.15.0",
    "prettier": "1.16.4",
    "sinon": "7.2.7",
    "ts-node": "8.0.2",
    "typescript": "3.3.3333"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
